#include <stdio.h>
#pragma warning (disable:4996)
void open(char c,int d)
{
    int i;
    char A[26],a[26],*ptr;
    for(i=0;i<26;i++)
    {
        A[i]='A'+i;
        a[i]='a'+i;
    }
    if((c>='a')&&(c<='z'))
    ptr=a;
    else
    ptr=A;
    for(i=1;i<=d;i++)
    printf("%c",ptr[(c-ptr[0]+i)%26]);
    
}

int main()
{
    char c;
    int d;
    printf("please input c&d\n");
    scanf("%c%d",&c,&d);
    open(c,d);
}
