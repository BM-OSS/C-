#include <stdio.h>
#include <math.h>
int powern(int a,register int b);
int main()
{
    int m;
    int x,y;
    printf("请输入a和b来求x的n次方\n");
    scanf("%d %d",&x,&y);
    m=powern(x,y);
    printf("其值为:%d",m);
    return 0;
}
int powern(int a,register int b)
{
    register int p=1;
    for(p=1;b;b--)
    {
        p*=a;
    }
    return p;
}
