#include <stdio.h>
typedef struct donet
{
   int data;
   struct donet *next;
}donet;
int main()
{
    donet a,b,c,*h,*p;
    a.data=10;
    b.data=20;
    c.data=30;
    h=&a;
    a.next=&b;
    b.next=&c;
    c.next='\0';
    p=h;
    while(p)
    {
        printf("%d\n",p->data);
        p=p->next;
        printf("\n");
    }    
}
