#include <stdio.h>
int main()
{
    for(int i=0;i<5;i++)
    {
        for(int j=i;j>=0;j--)
        {
            printf("*");
        }
        printf("\n");
    }
    for(int i=0;i<5;i++)
    {
        for(int j=5-i;j>0;j--)
        {
            printf("*");
        }
        printf("\n");
    }
    for(int i=0;i<5;i++)
    {
        for(int j=5-i;j>1;j--)
        {
            printf(" ");
        }
        for(int k=0;k<=i;k++)
        {
            printf("*");
        } 
        printf("\n");       
    }
    for(int i=0;i<5;i++)
    {
        for(int k=0;k<i;k++)
        {
            printf(" ");
        }
        for(int j=5-i;j>0;j--)
        {
            printf("*");
        }
        printf("\n");
    }
}
