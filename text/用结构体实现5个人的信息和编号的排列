#include <stdio.h>
#include <string.h>
#define M 5
typedef struct USER
{
    char name[50];
    char num[10];
}USER;
void getdata(USER *ps);
void sortdata(USER *ps);
void print(USER *ps);
int main()
{
    USER *ps;
    printf("请输入你5个数据\n");
    getdata(ps);
    printf("排序前的数组:\n");
    print(ps);
    sortdata(ps);
    printf("排序后的数组:\n");
    print(ps);
}
void getdata(USER *ps)
{
   for(int i=0;i<M;i++)
   {
      gets(ps[i].name);
      gets(ps[i].num);
   } 
}
void sortdata(USER *ps)
{
    for(int i=0;i<M;i++)
    {
        int m=0;
        for(int j=i+1;j<M;j++)
        {
            USER temp;
            if(strcmp(ps[j-1].num,ps[j].num)>0)
            {
                temp=ps[j-1];
                ps[j-1]=ps[j];
                ps[j]=temp;
                m=1;
            }           
        }
        if(m==0)
        {
            break;
        }
    }
}
void print(USER *ps)
{
    for(int i=0;i<M;i++)
    {
        puts(ps[i].name);
        puts(ps[i].num);
        printf("\n");
    }
}
