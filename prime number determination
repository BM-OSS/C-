#include <stdio.h>
int main()
{
   int i,n;
   printf ("plaese enter a positive integer\n");
   scanf("%d",&n);
   if(n<=1)
   {printf("not a prime number\n");}
   else
   {
    int flag=1;
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0)
        flag=0;
        break;
    }
    if(flag==1)
    {printf("is a prime number\n");}
    else
    printf("not a prime number\n");
   }
   return 0;
}
